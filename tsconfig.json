{
  "compilerOptions": {
    "sourceMap": true,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "outDir": "./dist",
    "strict": false,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "removeComments": false,
    "isolatedModules": false,
    "noEmit": false
  },
  "exclude": ["node_modules"]
}
